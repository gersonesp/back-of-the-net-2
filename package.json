{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "server": "nodemon ./bin/www",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "client": "npm start --prefix ./client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "debug": "nodemon  --inspect ./bin/www",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --exit --timeout 30000"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.5.8",
    "mongoose": "^5.9.18",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.2",
    "validator": "^13.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0"
  },
  "engines": {
    "node": "10.x"
  }
}
